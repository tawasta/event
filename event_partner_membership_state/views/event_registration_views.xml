<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.ui.view" id="view_event_registration_tree_membership_state">
        <field name="name">event.registration.tree.membership.state</field>
        <field name="model">event.registration</field>
        <field name="inherit_id" ref="event.view_event_registration_tree" />
        <field name="arch" type="xml">
            <field name="mobile" position="after">
                <field name="partner_membership_state" optional="hide" />
            </field>
        </field>
    </record>
    <record model="ir.ui.view" id="view_event_registration_form_membership_state">
        <field name="name">event.registration.form.membership.state</field>
        <field name="model">event.registration</field>
        <field name="inherit_id" ref="event.view_event_registration_form" />
        <field name="arch" type="xml">
            <field name="mobile" position="after">
                <field name="partner_membership_state" />
            </field>
        </field>
    </record>
</odoo>
