<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template
        id="website_event_events_list"
        inherit_id="website.snippets"
        name="Snippet Event"
    >
        <xpath expr="//t[@t-snippet='website_event.s_country_events']" position="after">
            <t
                t-snippet="website_event_events_snippet.s_events_list"
                t-thumbnail="/website_event/static/src/img/snippets_thumbs/s_country_events.svg"
            />
        </xpath>
    </template>
</odoo>
