#. module: event_notification_subscription
#: model:mail.template,body_html:event_notification_subscription.tag_based_notification_mail
msgid ""
"<p style=\"margin:0px 0 0px 0;box-sizing:border-box;margin-bottom: 0px;\">\n"
"                    Hello, \n"
"                    \n"
"                    <br><br>\n"
"\n"
"                    We have published a new event <strong style=\"box-sizing:border-box;font-weight:bolder;\"><t t-out=\"object.name or ''\">event name</t></strong> you might be interested in.\n"
"                    <br><br>\n"
"\n"
"                    <t t-set=\"tag_string\" t-value=\"', '.join([tag.name for tag in object.tag_ids])\"></t>\n"
"                    <ul>\n"
"                        <li>Event tags: <t t-out=\"tag_string or '-'\">tags</t></li>\n"
"                        <li>City: <t t-out=\"object.address_id and object.address_id.city or '-'\">city</t></li>\n"
"                        <li>Date: <t t-out=\"format_datetime(object.date_begin, dt_format='dd.MM.yyyy')\">date</t></li>\n"
"                    </ul>\n"
"                    <br>\n"
"\n"
"                    More details are available on the <a t-attf-href=\"{{object.website_url}}\" style=\"text-decoration: underline\">event page</a>.\n"
"\n"
"                    <br><br>\n"
"                    Best regards, <br>\n"
"                    - <t t-out=\"object.company_id.name\">company name</t>\n"
"                </p>                \n"
"            "
msgstr ""

