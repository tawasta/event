#. module: event_waiting_list
#: model:mail.template,body_html:event_waiting_list.event_waiting_registration
msgid ""
"<div>\n"
"                    <strong>Dear ${ctx.get('name')}</strong>,<br/>\n"
"                    We have more tickets available for: ${ctx.get('event')}.<br/>\n"
"                    Claim your spot from the following link: <a href=\"${ctx.get('confirm_url')}\"/><br/>\n"
"                    <div style=\"margin: 16px 0px 16px 0px;\">\n"
"                        <a href=\"${ctx.get('confirm_url')}\" style=\"background-color: #875A7B; padding: 8px 16px 8px 16px; text-decoration: none; color: #fff; border-radius: 5px; font-size:13px;\">\n"
"                            Claim your spot\n"
"                        </a>\n"
"                    </div>\n"
"                </div>\n"
"            "
msgstr ""
"<div>\n"
"                    <strong>Hei ${ctx.get('name')}</strong>,<br/>\n"
"                    Meillä on lisää lippuja tarjolla tapahtumaan: ${ctx.get('event')}.<br/>\n"
"                    Voit vahvistaa paikkasi seuraavan linkin kautta: <a href=\"${ctx.get('confirm_url')}\"/><br/>\n"
"                    <div style=\"margin: 16px 0px 16px 0px;\">\n"
"                        <a href=\"${ctx.get('confirm_url')}\" style=\"background-color: #875A7B; padding: 8px 16px 8px 16px; text-decoration: none; color: #fff; border-radius: 5px; font-size:13px;\">\n"
"                            Vahvista Paikkasi\n"
"                        </a>\n"
"                    </div>\n"
"                </div>\n"
"            "

#. module: event_waiting_list
#: model:mail.template,body_html:event_waiting_list.event_waiting
msgid ""
"<div>\n"
"                    Dear ${object.name},<br/>\n"
"                    Thank you for your inquiry.<br/>\n"
"                    The event: ${object.event_id.name}, is currently sold out.<br/>\n"
"                    You have been placed on a waiting list. We will contact you if any tickets are available.\n"
"                    If you have any questions, please let us know.\n"
"                    <br/><br/>\n"
"                    Thank you,\n"
"                    % if object.event_id.user_id.signature:\n"
"                    <br/>\n"
"                    ${object.event_id.user_id.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "
msgstr ""
"<div>\n"
"                    Hei ${object.name},<br/>\n"
"                    Kiitos kiinnostuksestasi tapahtumaan.<br/>\n"
"                    Tapahtuma: ${object.event_id.name}, on tällä hetkellä täynnä.<br/>\n"
"                    Sinut on laitettu jonotuslistalle. Otamme teihin yhteyttä, jos paikkoja vapautuu.\n"
"                    Jos teillä on jotain kysyttävää voitte ottaa meihin yhteyttä.\n"
"                    <br/><br/>\n"
"                    Kiitos,\n"
"                    % if object.event_id.user_id.signature:\n"
"                    <br/>\n"
"                    ${object.event_id.user_id.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "

