#. module: website_event_track_advanced
#: model:mail.template,body_html:website_event_track_advanced.mail_template_data_track_announced
msgid ""
"<table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" style=\"padding-top: 16px; background-color: #F1F1F1; font-family:Verdana, Arial,sans-serif; color: #454748; width: 100%; border-collapse:separate;\">\n"
"                <tr>\n"
"                    <td align=\"center\">\n"
"                        <t t-set=\"website_url\" t-value=\"object.get_base_url()\"></t>\n"
"                        <table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"590\" style=\"padding: 16px; background-color: white; color: #454748; border-collapse:separate;\">\n"
"                            <tbody>\n"
"                                <!-- HEADER -->\n"
"                                <tr>\n"
"                                    <td align=\"center\" style=\"min-width: 590px;\">\n"
"                                        <table width=\"590\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" style=\"min-width: 590px; background-color: white; padding: 0px 8px 0px 8px; border-collapse:separate;\">\n"
"                                            <tr>\n"
"                                                <td valign=\"middle\">\n"
"                                                    <span style=\"font-size: 10px;\">\n"
"                                                        Your proposal\n"
"                                                    </span>\n"
"                                                    <br>\n"
"                                                    <span style=\"font-size: 20px; font-weight: bold;\">\n"
"                                                        <t t-out=\"object.name\"></t>\n"
"                                                    </span>\n"
"                                                </td>\n"
"                                                <td valign=\"middle\" align=\"right\">\n"
"\n"
"                                                    <a t-attf-href=\"{{website_url}}/my/tracks\" style=\"padding: 8px 12px; font-size: 12px; color: #FFFFFF; text-decoration: none !important; font-weight: 400; background-color: #875A7B; border: 0px solid #875A7B; border-radius:3px\">\n"
"                                                        View Proposal\n"
"                                                    </a>\n"
"                                                </td>\n"
"                                            </tr>\n"
"                                            <tr>\n"
"                                                <td colspan=\"2\" style=\"text-align:center;\">\n"
"                                                    <hr width=\"100%\" style=\"background-color:rgb(204,204,204);border:medium none;clear:both;display:block;font-size:0px;min-height:1px;line-height:0; margin:16px 0px 16px 0px;\">\n"
"                                                </td>\n"
"                                            </tr>\n"
"                                        </table>\n"
"                                    </td>\n"
"                                </tr>\n"
"                                <!-- CONTENT -->\n"
"                                <tr>\n"
"                                    <td align=\"center\" style=\"min-width: 590px;\">\n"
"                                        <table width=\"590\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" style=\"min-width: 590px; background-color: white; padding: 0px 8px 0px 8px; border-collapse:separate;\">\n"
"                                            <tr>\n"
"                                                <td valign=\"top\" style=\"font-size: 14px;\">\n"
"                                                    <div>\n"
"                                                        <p>\n"
"                                                            Dear <t t-out=\"object.partner_id.name\"></t>,\n"
"                                                        </p><br>\n"
"                                                        <p>\n"
"                                                            Thank you for your proposal <strong><t t-out=\"object.name\"></t></strong> for the event <t t-out=\"object.event_id.name\"></t>.\n"
"                                                        </p>\n"
"                                                        <p>\n"
"                                                            Your proposal has been <strong>approved</strong> to the <t t-out=\"object.event_id.name\"></t> programme. Congratulations!\n"
"                                                        </p>\n"
"                                                        <p>\n"
"                                                            The proposal had an average rating of <strong><t t-out=\"object.rating_avg\"></t></strong> with scale of 1-5.\n"
"                                                        </p>\n"
"                                                        <br>\n"
"                                                        <p>Please take care of these:</p>\n"
"                                                        <ul>\n"
"                                                            <li>Check and edit your presentation contents. The final contents will be used in conference handbill and publications.</li>\n"
"                                                            <li>Confirm the presentation after you have done all the necessary edits.</li>\n"
"                                                            <li>Unconfirmed presentations will be left out from conference programme.</li>\n"
"                                                        </ul>\n"
"                                                        <t t-if=\"website_url\">\n"
"                                                        <br>\n"
"                                                        <p>\n"
"                                                            The proposal can be edited in\n"
"                                                            <a t-attf-href=\"{{website_url}}/my/tracks\" style=\"color:#875A7B;\">My Proposals</a>.\n"
"                                                        </p>\n"
"                                                        </t>\n"
"                                                        <br>\n"
"                                                        <div>\n"
"                                                            Welcome to the conference,<br>\n"
"                                                            <span style=\"color: #454748;\">\n"
"                                                            -- <br>\n"
"                                                            The <t t-out=\"object.event_id.name\"></t> Team\n"
"                                                            </span>\n"
"                                                        </div>\n"
"                                                    </div>\n"
"                                                </td>\n"
"                                            </tr>\n"
"                                            <tr>\n"
"                                                <td style=\"text-align:center;\">\n"
"                                                    <hr width=\"100%\" style=\"background-color:rgb(204,204,204);border:medium none;clear:both;display:block;font-size:0px;min-height:1px;line-height:0; margin: 16px 0px 16px 0px;\">\n"
"                                                </td>\n"
"                                            </tr>\n"
"                                        </table>\n"
"                                    </td>\n"
"                                </tr>\n"
"                            </tbody>\n"
"                        </table>\n"
"                    </td>\n"
"                </tr>\n"
"                <tr>\n"
"                    <td align=\"center\" style=\"min-width: 590px;\">\n"
"                        <t t-if=\"object.company_id\">\n"
"                            <table width=\"590\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" style=\"min-width: 590px; background-color: #F1F1F1; color: #454748; padding: 8px; border-collapse:separate;\">\n"
"                                <tr>\n"
"                                    <td style=\"text-align: center; font-size: 14px;\">\n"
"                                        Sent by\n"
"                                        <a target=\"_blank\" t-attf-href=\"{{object.company_id.website}}\" style=\"color: #875A7B;\">\n"
"                                            <t t-out=\"object.company_id.name\"></t>\n"
"                                        </a>\n"
"                                    </td>\n"
"                                </tr>\n"
"                            </table>\n"
"                        </t>\n"
"                    </td>\n"
"                </tr>\n"
"            </table>\n"
"        "
msgstr ""
"<table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" style=\"padding-top: 16px; background-color: #F1F1F1; font-family:Verdana, Arial,sans-serif; color: #454748; width: 100%; border-collapse:separate;\">\n"
"                    <tr>\n"
"                        <td align=\"center\">\n"
"                            % set website_url = object.env['ir.config_parameter'].sudo().get_param('web.base.url')\n"
"                            <table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"590\" style=\"padding: 16px; background-color: white; color: #454748; border-collapse:separate;\">\n"
"                                <tbody>\n"
"                                    <!-- HEADER -->\n"
"                                    <tr>\n"
"                                        <td align=\"center\" style=\"min-width: 590px;\">\n"
"                                            <table width=\"590\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" style=\"min-width: 590px; background-color: white; padding: 0px 8px 0px 8px; border-collapse:separate;\">\n"
"                                                <tr>\n"
"                                                    <td valign=\"middle\">\n"
"                                                        <span style=\"font-size: 10px;\">\n"
"                                                            Esitysehdotuksesi\n"
"                                                        </span>\n"
"                                                        <br/>\n"
"                                                        <span style=\"font-size: 20px; font-weight: bold;\">\n"
"                                                            ${object.name}\n"
"                                                        </span>\n"
"                                                    </td>\n"
"                                                    <td valign=\"middle\" align=\"right\">\n"
"                                                        % if website_url\n"
"                                                        <a href=\"${website_url}/my/tracks\" style=\"padding: 8px 12px; font-size: 12px; color: #FFFFFF; text-decoration: none !important; font-weight: 400; background-color: #875A7B; border: 0px solid #875A7B; border-radius:3px\">\n"
"                                                            Näytä ehdotus\n"
"                                                        </a>\n"
"                                                        % else\n"
"                                                        <img src=\"${'/logo.png?company=%s' % object.company_id.id}\" style=\"padding: 0px; margin: 0px; height: auto; width: 80px;\" alt=\"${'%s' % object.company_id.name}\"/>\n"
"                                                        % endif\n"
"                                                    </td>\n"
"                                                </tr>\n"
"                                                <tr>\n"
"                                                    <td colspan=\"2\" style=\"text-align:center;\">\n"
"                                                        <hr width=\"100%\" style=\"background-color:rgb(204,204,204);border:medium none;clear:both;display:block;font-size:0px;min-height:1px;line-height:0; margin:16px 0px 16px 0px;\"/>\n"
"                                                    </td>\n"
"                                                </tr>\n"
"                                            </table>\n"
"                                        </td>\n"
"                                    </tr>\n"
"                                    <!-- CONTENT -->\n"
"                                    <tr>\n"
"                                        <td align=\"center\" style=\"min-width: 590px;\">\n"
"                                            <table width=\"590\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" style=\"min-width: 590px; background-color: white; padding: 0px 8px 0px 8px; border-collapse:separate;\">\n"
"                                                <tr>\n"
"                                                    <td valign=\"top\" style=\"font-size: 14px;\">\n"
"                                                        <div>\n"
"                                                            <p>\n"
"                                                                Hyvä ${object.partner_id.name},\n"
"                                                            </p><br/>\n"
"                                                            <p>\n"
"                                                                Kiitos esitysehdotuksestasi <strong>${object.name}</strong> tapahtumaan ${object.event_id.name}.\n"
"                                                            </p>\n"
"                                                            <p>\n"
"                                                                Ehdotuksesi on <strong>hyväksytty</strong> tapahtuman ${object.event_id.name} ohjelmaan. Onnittelut!\n"
"                                                            </p>\n"
"                                                            <p>\n"
"                                                                Ehdotuksesi arvioinnin keskiarvo oli <strong>${object.rating_avg}</strong> asteikolla 1-5.\n"
"                                                            </p>\n"
"                                                            <br/>\n"
"                                                            <p>Huolehdi näistä:</p>\n"
"                                                            <ul>\n"
"                                                                <li>Tarkasta ja muokkaa esityksen sisältö. Lopullista sisältöä voidaan käyttää tapahtuman käsiohjelmassa ja julkaisuissa.</li>\n"
"                                                                <li>Vahvista esitys kun olet tehnyt tarvittavat muutokset.</li>\n"
"                                                                <li>Vahvistamattomat esitykset jätetään pois tapahtuman ohjelmasta.</li>\n"
"                                                            </ul>\n"
"                                                            % if 'website_url':\n"
"                                                            <br/>\n"
"                                                            <p>\n"
"                                                                Voit muokata ehdotustasi\n"
"                                                                <a href=\"${website_url}/my/tracks\" style=\"color:#875A7B;\">Minun Esityksistä</a>.\n"
"                                                            </p>\n"
"                                                            % endif\n"
"                                                            <br/>\n"
"                                                            <div>\n"
"                                                                Tervetuloa tapahtumaan,<br/>\n"
"                                                                <span style=\"color: #454748;\">\n"
"                                                                -- <br/>\n"
"                                                                Tapahtuman ${object.event_id.name} tiimi\n"
"                                                                </span>\n"
"                                                            </div>\n"
"                                                        </div>\n"
"                                                    </td>\n"
"                                                </tr>\n"
"                                                <tr>\n"
"                                                    <td style=\"text-align:center;\">\n"
"                                                        <hr width=\"100%\" style=\"background-color:rgb(204,204,204);border:medium none;clear:both;display:block;font-size:0px;min-height:1px;line-height:0; margin: 16px 0px 16px 0px;\"/>\n"
"                                                    </td>\n"
"                                                </tr>\n"
"                                            </table>\n"
"                                        </td>\n"
"                                    </tr>\n"
"                                </tbody>\n"
