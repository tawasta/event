<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="event_track_reviewers_action" model="ir.actions.act_window" >
        <field name="name">Track reviewers</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">res.partner</field>
        <field name="view_type">form</field>
        <field name="domain">[('review_group_ids', '!=', False)]</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="website_event_track_advanced.view_event_track_reviewer_tree"/>
        <field name="view_ids" eval="[(5, 0, 0),
            (0, 0, {'view_mode': 'tree', 'view_id': ref('website_event_track_advanced.view_event_track_reviewer_tree')}),
            ]"/>
    </record>

    <menuitem id="event_track_reviewers_menu"
              name="Track reviewers"
              action="event_track_reviewers_action"
              sequence="5"
              parent="website_event_track_advanced.menu_event_track_reviews"
              groups="event.group_event_manager"
              />

</odoo>