<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_event_track_rating_tree" model="ir.ui.view">
        <field name="name">event_track_rating_tree</field>
        <field name="model">event.track.rating</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">

            <tree string="Event track" create="false" delete="false">
                <field name="event_id"/>
                <field name="event_track"/>
                <field name="create_uid"/>
                <field name="create_date"/>
                <field name="rating"/>
                <field name="comment"/>
                <field name="active" invisible="1" />
            </tree>

        </field>
    </record>

</odoo>