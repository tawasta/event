<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_event_track_subtheme_tree" model="ir.ui.view">
        <field name="name">event.track.subtheme.tree</field>
        <field name="model">event.track.subtheme</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>
    <record id="view_event_track_subtheme_form" model="ir.ui.view">
        <field name="name">event.track.subtheme.form</field>
        <field name="model">event.track.subtheme</field>
        <field name="arch" type="xml">
            <form string="Track Type">
                <header />
                <sheet string="Track Type">
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="event_track_subtheme_action" model="ir.actions.act_window">
        <field name="name">Track subthemes</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">event.track.subtheme</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
