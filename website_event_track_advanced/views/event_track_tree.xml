<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record id="view_event_track_advanced_tree" model="ir.ui.view">
			<field name="name">event_track_advanced_tree</field>
			<field name="model">event.track</field>
            <field name="type">tree</field>
            <field name="priority" eval="20" />
			<field name="arch" type="xml">

                <tree string="Event Track">
                    <field name="name"/>
                    <field name="partner_id"/>
                    <field name="state"/>
                    <field name="language"/>
                    <field name="review_group"/>
                    <field name="rating_avg"/>
                    <field name="active" invisible="1"/>
                </tree>

			</field>
		</record> 

	</data>
</openerp>